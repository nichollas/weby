<% title ".users" %>
<% breadcrumb :users %>
<p class="pull-right">
  <% if check_permission(Admin::RolesController, [:index]) %>
    <%= link_to t(".roles_permissions"),
      main_app.admin_roles_path,
      class: "btn btn-default" %>
  <% end %>
</p>
<div class="clearfix"></div>

<div id="enroled">
  <%= render 'enroled' %>
</div>
<div id="enrole">
  <%= render 'enrole' %>
</div>

